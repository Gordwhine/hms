            <div class="bg-holder bg-size" style="background-image:url(img/gallery/dot-bg.png);background-position:bottom right;background-size:auto;"></div>
            <!--/.bg-holder-->
            <div class="col-lg-6 z-index-2 mb-5"><img class="w-100" src="img/gallery/appointment.png" alt="..." /></div>
            <div class="col-lg-6 z-index-2">
              <form class="row g-3">
                <div class="col-md-6"><label class="visually-hidden" for="inputName">Name</label><input class="form-control form-livedoc-control" id="inputName" type="text" placeholder="Name" /></div>
